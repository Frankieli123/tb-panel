# Database
DATABASE_URL="postgresql://user:password@localhost:5432/taobao_tracker"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
PORT=4000
NODE_ENV=development

# API Key (可选，设置后所有API请求需要携带此密钥)
# 前端需要配置相同的密钥
API_KEY=

# Auth
# 首次初始化管理员的邀请码（系统内尚无用户时生效）
BOOTSTRAP_INVITE_CODE=
# Session cookie 名称
SESSION_COOKIE_NAME=tbpanel_session
# Session 有效期（天）
SESSION_TTL_DAYS=30

# Email (SMTP)
SMTP_HOST=smtp.qq.com
SMTP_PORT=465
SMTP_USER=your_email@qq.com
SMTP_PASS=your_smtp_password
SMTP_FROM=your_email@qq.com

# WeChat (Server酱 or 企业微信)
WECHAT_WEBHOOK_URL=

# Scraper Settings
SCRAPER_MIN_INTERVAL_MS=60000
SCRAPER_MAX_INTERVAL_MS=180000
SCRAPER_PAGE_TIMEOUT_MS=30000
MAX_CONCURRENT_ACCOUNTS=3
